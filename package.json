{
  "name": "persik-tv-app",
  "version": "2.1.8",
  "main": "main.js",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "http": false,
    "https": false
  },
  "browserslist": [
    "and_chr 97",
    "and_ff 95",
    "and_qq 10.4",
    "and_uc 12.12",
    "baidu 7.12",
    "chrome 97",
    "chrome 96",
    "chrome 95",
    "chrome 94",
    "edge 97",
    "edge 96",
    "firefox 96",
    "firefox 95",
    "firefox 94",
    "firefox 91",
    "firefox 78",
    "ie 11",
    "ios_saf 15.0-15.1",
    "ios_saf 14.0-14.4",
    "kaios 2.5",
    "op_mini all",
    "op_mob 64",
    "opera 82",
    "safari 13.1"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "build": "ng build --configuration production",
    "electron": "ng build && electron .",
    "packager:win": "electron-packager . PersikApp --platform=win32 --arch=all",
    "packager:win-release": "npm run build && electron-packager . PersikApp --platform=win32 --arch=x64 --out=release-builds --no-prune --overwrite",
    "packager:mac": "electron-packager . PersikApp --platform=darwin --arch=x64",
    "packager:mac-release": "ng build && electron-packager . PersikApp --platform=darwin --arch=x64 --out=release-builds",
    "watch": "nodemon -e ts -w ./src -x npm run electron",
    "build-all": "ng build && npm run packager:win-release && npm run packager:mac-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.2",
    "@angular/cdk": "^13.2.6",
    "@angular/common": "~13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/core": "~13.1.2",
    "@angular/forms": "~13.1.2",
    "@angular/material": "^13.1.2",
    "@angular/platform-browser": "~13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/router": "~13.1.2",
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngrx/store-devtools": "^13.0.2",
    "angular-epic-spinners": "^2.0.0",
    "angular-material-icons": "^0.7.1",
    "angularx-qrcode": "^12.0.3",
    "core-js": "^2.5.4",
    "electron-is-dev": "^2.0.0",
    "electron-window-state": "^5.0.3",
    "hls.js": "0.7.11",
    "jquery": "^3.4.1",
    "mat-icon-import": "^0.1.1",
    "material-icons": "^1.11.11",
    "moment": "^2.24.0",
    "ngx-infinite-scroll": "^7.2.0",
    "rxjs": "~6.6.7",
    "scroll-into-view": "^1.9.7",
    "tslib": "^2.0.0",
    "update": "^0.7.4",
    "zone.js": "~0.11.4"
